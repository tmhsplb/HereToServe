<div class="form-group">
    <div class="col-md-2">
        @Html.CheckBoxFor(m => m.NewTDL, new { @class = "form-control" })
    </div>
    <div class="col-md-2">
        @Html.LabelFor(m => m.NewTDL, new { @class = "control-label" })
    </div>
    <div class="col-md-2">
        @Html.CheckBoxFor(m => m.ReplacementTDL, new { @class = "form-control" })
    </div>
