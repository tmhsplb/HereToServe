    <div class="col-md-2">
        @Html.CheckBoxFor(m => m.Numident, new { @class = "form-control" })
    </div>
    <div class="col-md-2">
        @Html.LabelFor(m => m.Numident, new { @class = "control-label" })
    </div>
    <div class="col-md-4">
        @Html.TextBoxFor(m => m.RequestedDocument, new { placeholder = "Requested Document", @class = "form-control" })
    </div>
</div>
