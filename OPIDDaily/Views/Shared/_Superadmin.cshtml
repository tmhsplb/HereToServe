<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Here to Serve</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/DataTables/css")

    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/mvc5datatables")
    @RenderSection("scripts", required: false)
</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top navbar-custom">
        <div class="container">
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Home", "Superadmin")</li>
                    <li>@Html.ActionLink("Users", "ManageUsers", "Users")</li>
                    <li>@Html.ActionLink("Roles", "ManageRoles", "Role")</li>
                    <li>@Html.ActionLink("Agencies", "ManageAgencies", "Agency")</li>
                    <li>@Html.ActionLink("MBVD", "ManageMBVDS", "MBVD")</li>
                    <li>@Html.ActionLink("Clients", "ManageServiceDateClients", "Superadmin")</li>
                    <li>@Html.ActionLink("History", "History", "Superadmin")</li>
                    <li>@Html.ActionLink("Rebuild", "Rebuild", "Superadmin")</li>
                    <li>@Html.ActionLink("Demo", "Demo", "Superadmin")</li>
                    <li>@Html.ActionLink("Recent Checks", "RecentChecks", "Superadmin")</li>
                    <li>@Html.ActionLink("Ancient Checks", "AncientChecks", "Superadmin")</li>
                    @*  <li>@Html.ActionLink("Pre-Approval Form", "PreApprovalForm", "Superadmin")</li> *@
                    @*@<li>@Html.ActionLink("Review", "ServiceDateReview", "Superadmin")</li>*@
                    <li>@Html.ActionLink("Version", "Version", "Superadmin")</li>
                </ul>
                @Html.Partial("_LoginPartial")
                @Html.Partial("_EnvironmentPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        @Html.Partial("_Footer")
    </div>
</body>
</html>
